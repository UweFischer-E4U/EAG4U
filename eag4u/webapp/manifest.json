{
    "_version": "1.59.0",
    "sap.app": {
        "id": "eag4u",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.12.0",
            "toolsId": "cb4a19a2-ad5d-4fbf-89dc-402b0b710f89"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZFK_SERVICES_FIORI_APP_V2/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "SEGWService": {
                "uri": "/sap/opu/odata/sap/ZFK_FIORI_SERVICES/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": true,
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.120.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "eag4u.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "RAPmodel": {
                "dataSource": "mainService",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "useBatch": false
                }
            },
            "SEGWmodel": {
                "dataSource": "SEGWService",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "useBatch": false
                }
            }
        },
        "resources": {
            "js": 
			[
				{
					"uri": "util/Formatter.js"
				}				
			],
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "eag4u.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteView1",
                    "pattern": ":?query:",
                    "target": [
                        "start"
                    ]
                },
                {
                    "pattern": "",
                    "name": "Start",
                    "target": "start"
                },
                {
                    "pattern": "view1",
                    "name": "View1",
                    "target": "View1"
                },
                {
                    "pattern": "Listen",
                    "name": "Listen",
                    "target": "listen"
                },
                {
                    "pattern": "Bearbeitung",
                    "name": "Bearbeitung",
                    "target": "bearbeitung"
                }
            ],
            "targets": {
                "TargetView1": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "View1",
                    "viewName": "View1"
                },
                "View1": {
                    "viewId": "View1",
                    "viewName": "View1"
                },
                "start": {
                    "viewId": "start",
                    "viewName": "Startseite"
                },
                "listen": {
                    "viewId": "listen",
                    "viewName": "Listen"
                },
                "bearbeitung": {
                    "viewId": "bearbeitung",
                    "viewName": "Bearbeitung"
                }
            }
        },
        "rootView": {
            "viewName": "eag4u.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "eag4u"
    }
}
